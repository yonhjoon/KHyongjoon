<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper >

<!-- namespace : 해당 mapper파일의 고유한 별칭 -->
<mapper namespace="memberMapper">

<!-- 
	*SELECT문일 경우
	<select id ="각 sql문의 식별자"
 -->
 <select id="selectBlog" resultType="Blog">
 	select * from Blog where id = #{id}
 </select>
</mapper>