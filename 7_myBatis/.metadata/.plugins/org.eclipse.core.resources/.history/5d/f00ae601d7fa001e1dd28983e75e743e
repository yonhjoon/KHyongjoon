package com.kh.mybatis.board.model.vo;

import java.sql.Date;

public class Reply {
	private int replyNo;
	private String repltContent;
	private int pefBno;
	private String replyWriter; //ID도 받아올수있으니 String로 만듬
	private Date createDate;
	private String status;
	
	public Reply() {
		super();
	}

	public Reply(int replyNo, String repltContent, int pefBno, String replyWriter, Date createDate, String status) {
		super();
		this.replyNo = replyNo;
		this.repltContent = repltContent;
		this.pefBno = pefBno;
		this.replyWriter = replyWriter;
		this.createDate = createDate;
		this.status = status;
	}

	public int getReplyNo() {
		return replyNo;
	}

	public void setReplyNo(int replyNo) {
		this.replyNo = replyNo;
	}

	public String getRepltContent() {
		return repltContent;
	}

	public void setRepltContent(String repltContent) {
		this.repltContent = repltContent;
	}

	public int getPefBno() {
		return pefBno;
	}

	public void setPefBno(int pefBno) {
		this.pefBno = pefBno;
	}

	public String getReplyWriter() {
		return replyWriter;
	}

	public void setReplyWriter(String replyWriter) {
		this.replyWriter = replyWriter;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	@Override
	public String toString() {
		return "Reply [replyNo=" + replyNo + ", repltContent=" + repltContent + ", pefBno=" + pefBno + ", replyWriter="
				+ replyWriter + ", createDate=" + createDate + ", status=" + status + "]";
	}
	
	
}
